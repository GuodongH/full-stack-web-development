{
    "title": "全栈技能修炼",
    "author": "王芃",
    "description": "Spring Boot 2.x 和 Angular 6.x 全栈技能打造效率工具",
    "language": "zh-hans",
    "gitbook": ">=3.2.3",
    "root": ".",
    "plugins": [
        "toc",
        "search-plus",
        "github",
        "katex",
        "mermaid-gb3",
        "puml",
        "graph",
        "donate",
        "terminal",
        "-sharing",
        "sharing-plus",
        "image-captions"
    ],
    "pluginsConfig": {
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "github": {
            "url": "https://github.com/wpcfan"
        },
        "donate": {
            "wechat": "/assets/donate/wechat.png",
            "alipay": "/assets/donate/alipay.png",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        },
        "terminal": {
            "copyButtons": true,
            "fade": false,
            "style": "flat"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook",
                "google",
                "twitter",
                "weibo",
                "instapaper",
                "linkedin",
                "pocket",
                "stumbleupon",
                "qq",
                "qzone"
            ]
        },
        "image-captions": {
            "caption": "_CAPTION_"
        }
    }
}
